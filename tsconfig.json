{
  "compilerOptions": {
    "lib": [
      "dom",
      "es2015",
      "es6"
    ],
    "module": "commonjs",
    "target": "es5",
    "declaration": false,
    "strict": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "./build/js",
    "rootDir": ".",
    "strictNullChecks": false,
//    "noEmit": true,
    "jsx": "react",
    "jsxFactory": "jsx"
  },
  "include": [
    "./src/jsx-types.d.ts",
    "./test/**/*.ts",
    "./test/**/*.tsx"
  ]
}
